{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window", "esnext"],
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "types": ["deno"],
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleResolution": "bundler"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}